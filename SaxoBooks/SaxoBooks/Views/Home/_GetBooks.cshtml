@model IEnumerable<SaxoBooks.Data.Models.Book>
<div id="book-results" class="col-md-8 books">
    @if (@Model != null)
    {
        foreach (var book in @Model)
        {
            <div class="book-image-wrapper">
                <img src="@book.ImageUrl"/>
            </div>
            <div class="book-isbn-wrapper">
                Book ISBN: @book.Isbn
            </div>
        }
    }
</div>